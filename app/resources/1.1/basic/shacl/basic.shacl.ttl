<#IntellectualEntityShape> a sh:NodeShape ;
  sh:targetClass premis:IntellectualEntity ;
  sh:closed true ;
  sh:property <#propertyTypeShape>,
  [
    a sh:PropertyShape ;
    sh:path premis:identifier ;
    sh:or (
      [ sh:class haObjId:MEEMOO-LOCAL-ID ]
      [ sh:class haObjId:Acquisition_number ]
      [ sh:class haObjId:Alternative_number ]
      [ sh:class haObjId:Analoge_drager ]
      [ sh:class haObjId:Ardome ]
      [ sh:class haObjId:Basis ]
      [ sh:class haObjId:Historical_carrier ]
      [ sh:class haObjId:Historical_record_number ]
      [ sh:class haObjId:Inventarisnummer ]
      [ sh:class haObjId:MEDIA_ID ]
      [ sh:class haObjId:Object_number ]
      [ sh:class haObjId:Priref ]
      [ sh:class haObjId:Word_ID ]
      [ sh:class haObjId:Topstuk_ID ]
      [ sh:class haObjId:WorkPID ]
      [ sh:class haObjId:DataPID ]
      [ sh:class haObjId:Bestandsnaam ]
      [ sh:class haObjId:Api ]
      [ sh:class haObjId:Pdf ]
      [ sh:class haObjId:PersistenteURI_Record ]
      [ sh:class haObjId:PersistenteURI_Werk ]
      [ sh:class haObjId:PersistenteURI_VKC_Record ]
      [ sh:class haObjId:PersistenteURI_VKC_Werk ]
      [ sh:class haObjId:MEEMOO-PID ]
    );
    sh:message """Class of premis:identifier is not part of allowed list.
      Please check `premis:premis/premis:object/premis:objectIdentifier/premis:objectIdentifierType`."""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path relSubType:isr ;
    sh:nodeKind sh:IRI ;
    sh:class premis:Representation ;
    # sh:node <#RepresentationShape> ;
    sh:name "is represented by"@en ;
    sh:description "The representation of the intellectual entity."@en ;
    sh:minCount 1 ;
    sh:severity sh:Violation ;
    sh:message """No representation or mismatch between IE's `objectIdentifier` and Representation's `objectIdentifier` and their relationships.
      Please check the identifiers in `premis:premis/premis:object/premis:relationship/premis:relatedObjectIdentifier`."""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:title ;
    sh:name "has title"@en ;
    sh:description "The title of the intellectual entity."@en ;
    sh:minCount 1 ;
    sh:severity sh:Violation ;
    sh:message """Intellectual entity has no `dcterms:title`.
      Please check `metadata/dcterms:title[@xml:lang=*]`."""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:title ;
    sh:nodeKind sh:Literal ;
    sh:name "has title"@en ;
    sh:description "The title of the intellectual entity."@en ;
    sh:severity sh:Violation ;
    sh:datatype rdf:langString;
    sh:message """`dcterms:title` is not a string or has no `@xml:lang` attribute.
      Please check `metadata/dcterms:title[@xml:lang=*]`."""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:title ;
    sh:name "has title"@en ;
    sh:description "The title of the intellectual entity."@en ;
    sh:severity sh:Violation ;
    sh:uniqueLang true ;
    sh:message """Multiple values for `dcterms:title` with the same `@xml:lang` attribute.
      Please check `metadata/dcterms:title[@xml:lang=*]`."""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:alternative ;
    sh:nodeKind sh:Literal ;
    sh:datatype rdf:langString;
    sh:name "has alternative title"@en ;
    sh:description "An alternative title of the intellectual entity."@en ;
    sh:severity sh:Violation ;
    sh:message """`dcterms:alternative` is not a string or has no `@xml:lang` attribute.
      Please check `metadata/dcterms:alternative[@xml:lang=*]`."""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:extent ;
    sh:nodeKind sh:Literal ;
    sh:name "has extent"@en ;
    sh:description "The extent of the intellectual entity."@en ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:message """More than one value for `dcterms:extent`.
      Please check `metadata/dcterms:extent`."""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:extent ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:duration ;
    sh:name "has extent"@en ;
    sh:description "The extent of the intellectual entity."@en ;
    sh:severity sh:Violation ;
    sh:message """`dcterms:extent` is not of type `xsd:duration` or is not in the right format.
      Please check `metadata/dcterms:extent`.
      The format of xsd:duration is PnYnMnDTnHnMnS, where P is a literal value that starts the expression, nY is the number of years followed by a literal Y, nM is the number of months followed by a literal M, nD is the number of days followed by a literal D, T is a literal value that separates the date and time, nH is the number of hours followed by a literal H, nM is the number of minutes followed by a literal M, and nS is the number of seconds followed by a literal S."""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:available ;
    sh:nodeKind sh:Literal ;
    sh:name "is available"@en ;
    sh:description "The datetime on which the intellectual entity is available."@en ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:message """More than one value for `dcterms:available`.
      Please check `metadata/dcterms:available`."""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:available ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:dateTime ;
    sh:name "is available"@en ;
    sh:description "The datetime on which the intellectual entity is available."@en ;
    sh:severity sh:Violation ;
    sh:message """`dcterms:available` is not of type `xsd:dateTime` or is not in the right format.
      Please check `metadata/dcterms:available`.
      The type xsd:dateTime represents a specific date and time in the format CCYY-MM-DDThh:mm:ss.sss, which is a concatenation of the date and time forms, separated by a literal letter "T"."""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:description ;
    sh:name "has description"@en ;
    sh:description "The description of the intellectual entity."@en ;
    sh:minCount 1 ;
    sh:severity sh:Violation ;
    sh:message """Intellectual entity has no `dcterms:description`.
      Please check `metadata/dcterms:description`."""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:description ;
    sh:nodeKind sh:Literal ;
    sh:datatype rdf:langString;
    sh:name "has description"@en ;
    sh:description "The description of the intellectual entity."@en ;
    sh:severity sh:Violation ;
    sh:message """`dcterms:description` is not a string or has no `@xml:lang` attribute.
      Please check `metadata/dcterms:description[@xml:lang=*]`"""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:description ;
    sh:name "has description"@en ;
    sh:description "The description of the intellectual entity."@en ;
    sh:uniqueLang true ;
    sh:severity sh:Violation ;
    sh:message """Multiple values for `dcterms:description` with the same `@xml:lang` attribute.
      Please check `metadata/dcterms:description[@xml:lang=*]`"""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:abstract ;
    sh:nodeKind sh:Literal ;
    sh:datatype rdf:langString;
    sh:name "has abstract"@en ;
    sh:description "The abstract of the intellectual entity."@en ;
    sh:severity sh:Violation ;
    sh:message """`dcterms:abstract` is not a string or has no `@xml:lang` attribute.
      Please check `metadata/dcterms:abstract[@xml:lang=*]`"""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:abstract ;
    sh:uniqueLang true ;
    sh:name "has abstract"@en ;
    sh:description "The abstract of the intellectual entity."@en ;
    sh:severity sh:Violation ;
    sh:message """Multiple values for `dcterms:abstract` with the same `@xml:lang` attribute.
      Please check `metadata/dcterms:abstract[@xml:lang=*]`"""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:created ;
    sh:nodeKind sh:Literal ;
    sh:name "has creation date"@en ;
    sh:description "The creation date of the intellectual entity."@en ;
    sh:minCount 1 ;
    sh:severity sh:Violation ;
    sh:message """Intellectual entity has no `dcterms:created`.
      Please check `metadata/dcterms:created`"""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:created ;
    sh:nodeKind sh:Literal ;
    sh:name "has creation date"@en ;
    sh:description "The creation date of the intellectual entity."@en ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:message """More than one value for `dcterms:created`.
      Please check `metadata/dcterms:created`"""@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:created ;
    sh:nodeKind sh:Literal ;
    sh:or (
      [ sh:datatype edtf:EDTF-level0 ]
      [ sh:datatype edtf:EDTF-level1 ]
      [ sh:datatype edtf:EDTF-level2 ]
      [ sh:datatype edtf:EDTF ]
    ) ;
    sh:name "has creation date"@en ;
    sh:description "The creation date of the intellectual entity."@en ;
    sh:severity sh:Violation ;
    sh:message "`dcterms:created` not of datatype EDTF."@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:issued ;
    sh:name "has issue date"@en ;
    sh:description "The issue date of the intellectual entity."@en ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:message "More than one value for `dcterms:issued`."@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:issued ;
    sh:nodeKind sh:Literal ;
    sh:or (
      [ sh:datatype edtf:EDTF-level0 ]
      [ sh:datatype edtf:EDTF-level1 ]
      [ sh:datatype edtf:EDTF-level2 ]
      [ sh:datatype edtf:EDTF ]
    ) ;
    sh:name "has issue date"@en ;
    sh:description "The issue date of the intellectual entity."@en ;
    sh:severity sh:Violation ;
    sh:message "`dcterms:issued` not of datatype EDTF."@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:publisher ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:string ;
    sh:name "has publisher"@en ;
    sh:description "The publisher of the intellectual entity."@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:contributor ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:string ;
    sh:name "has contributor"@en ;
    sh:description "The contributor of the intellectual entity."@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:creator ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:string ;
    sh:name "has creator"@en ;
    sh:description "The creator of the intellectual entity."@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:spatial ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:string ;
    sh:name "has spatial coverage"@en ;
    sh:description "The spatial coverage of the intellectual entity."@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:temporal ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:string ;
    sh:name "has temporal coverage"@en ;
    sh:description "The temporal coverage of the intellectual entity."@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:subject ;
    sh:NodeKind sh:Literal ;
    sh:minCount 1 ;
    sh:uniqueLang false ;
    sh:datatype rdf:langString ;
    sh:name "has subject"@en ;
    sh:description "The subject or keywords of the intellectual entity."@en ;
    sh:severity sh:Warning ;
    sh:message "Intellectual entity has no values for `dcterms:subject`."@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:language ;
    sh:nodeKind sh:Literal ;
    sh:minCount 1 ;
    sh:datatype xsd:string ;
    sh:name "has language"@en ;
    sh:description "The language of the intellectual entity."@en ;
    sh:severity sh:Warning ;
    sh:message "Intellectual entity has no values for `dcterms:language`."@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:license ;
    sh:nodeKind sh:Literal ;
    sh:minCount 1 ;
    sh:datatype xsd:string ;
    sh:name "has license"@en ;
    sh:description "The license of the intellectual entity."@en ;
    sh:severity sh:Warning ;
    sh:message "Intellectual entity has no values for `dcterms:license`."@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:rightsHolder ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:string ;
    sh:name "has rights holder"@en ;
    sh:description "The rights holder of the intellectual entity."@en ;
    sh:minCount 1 ;
    sh:severity sh:Warning ;
    sh:message "Intellectual entity has no values for `dcterms:rightsHolder`."@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:rightsHolder ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:string ;
    sh:name "has rights holder"@en ;
    sh:description "The rights holder of the intellectual entity."@en ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:message "More than one value for `dcterms:rightsHolder`."@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:rights ;
    sh:nodeKind sh:Literal ;
    sh:datatype rdf:langString ;
    sh:name "has rights"@en ;
    sh:description "The rights of the intellectual entity."@en ;
    sh:minCount 1 ;
    sh:severity sh:Warning ;
    sh:message "Intellectual entity has no values for `dcterms:rights`."@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:rights ;
    sh:nodeKind sh:Literal ;
    sh:datatype rdf:langString ;
    sh:uniqueLang true ;
    sh:name "has rights"@en ;
    sh:description "The rights of the intellectual entity."@en ;
    sh:severity sh:Violation ;
    sh:message "Multiple values for `dcterms:rights` with the same `@xml:lang` attribute."@en ;
  ],
  [
    a sh:PropertyShape ;
    sh:path dcterms:type ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:string ;
    sh:name "has type"@en ;
    sh:description "The classification of the intellectual entity."@en ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:message "More than one value for `dcterms:type`."@en ;
  ] .

